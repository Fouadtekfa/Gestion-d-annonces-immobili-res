extends layout

block content
  doctype html
  head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Document
    link(rel='stylesheet' href='./css/bootstrap.min.css')
    link(rel='stylesheet' href='./stylesheets/style.css')
  .nav-screen
    nav.navbar.navbar-expand-md.navbar-dark.bg-dark
      .container-fluid
        button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarTogglerDemo01' aria-controls='navbarTogglerDemo01' aria-expanded='false' aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarTogglerDemo01.collapse.navbar-collapse
            ul.navbar-nav.me-auto.mb-2.mb-lg-0
              li.nav-item.active
                a.nav-link(href=default_directory) Home 
            ul.navbar-nav.my-2.my-lg-0
              li.nav-item
                a.nav-link(href=default_directory+'/login') Connexion
              li.nav-item
                a.nav-link(href=default_directory+'/signup') Cr&eacute;er un compte
  header
    .containter-gral-header
      .image
      .content-container
        .right-text
          .txt-content
            | Paris Immobilier
  script(src='./js/jquery.slim.min.js')
  script(src='./js/popper.js')
  script(src='./js/bootstrap.js')
  script(src='./javascripts/home.js')

